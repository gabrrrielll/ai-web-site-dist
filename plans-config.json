{
    "isEditable": true,
    "show_import_export_config": false,
    "useLocal_site-config": false,
    "show_save_button": false,
    "services": {
        "ai_image_generation": {
            "name": "AI Image Generation",
            "description": "Generate images using AI from text prompts",
            "limits": {
                "localhost": {
                    "type": "unlimited",
                    "value": null,
                    "description": "Unlimited usage on localhost"
                },
                "test_domain": {
                    "type": "daily_limit",
                    "value": 3,
                    "description": "3 images per day for test domains"
                },
                "public_domain": {
                    "type": "unlimited",
                    "value": null,
                    "description": "Unlimited usage for public domains"
                }
            },
            "enabled": true,
            "provider": "pollinations_ai",
            "fallback_provider": "canvas_picsum"
        },
        "ai_text_generation": {
            "name": "AI Text Generation",
            "description": "Generate articles and text content using AI",
            "limits": {
                "localhost": {
                    "type": "unlimited",
                    "value": null,
                    "description": "Unlimited usage on localhost"
                },
                "test_domain": {
                    "type": "daily_limit",
                    "value": 5,
                    "description": "5 articles per day for test domains"
                },
                "public_domain": {
                    "type": "unlimited",
                    "value": null,
                    "description": "Unlimited usage for public domains"
                }
            },
            "enabled": true,
            "provider": "google_gemini"
        },
        "ai_seo_generation": {
            "name": "AI SEO Generation",
            "description": "Generate SEO metadata and descriptions",
            "limits": {
                "localhost": {
                    "type": "unlimited",
                    "value": null,
                    "description": "Unlimited usage on localhost"
                },
                "test_domain": {
                    "type": "daily_limit",
                    "value": 10,
                    "description": "10 SEO generations per day for test domains"
                },
                "public_domain": {
                    "type": "unlimited",
                    "value": null,
                    "description": "Unlimited usage for public domains"
                }
            },
            "enabled": true,
            "provider": "google_gemini"
        },
        "image_optimization": {
            "name": "Image Optimization",
            "description": "Compress and optimize uploaded images",
            "limits": {
                "localhost": {
                    "type": "unlimited",
                    "value": null,
                    "description": "Unlimited usage on localhost"
                },
                "test_domain": {
                    "type": "daily_limit",
                    "value": 20,
                    "description": "20 image optimizations per day for test domains"
                },
                "public_domain": {
                    "type": "unlimited",
                    "value": null,
                    "description": "Unlimited usage for public domains"
                }
            },
            "enabled": true,
            "provider": "browser_canvas"
        },
        "export_functionality": {
            "name": "Export Functionality",
            "description": "Export site as static files or ZIP",
            "limits": {
                "localhost": {
                    "type": "unlimited",
                    "value": null,
                    "description": "Unlimited usage on localhost"
                },
                "test_domain": {
                    "type": "daily_limit",
                    "value": 2,
                    "description": "2 exports per day for test domains"
                },
                "public_domain": {
                    "type": "unlimited",
                    "value": null,
                    "description": "Unlimited usage for public domains"
                }
            },
            "enabled": true,
            "provider": "browser_fs"
        }
    },
    "domain_types": {
        "localhost": {
            "pattern": "localhost",
            "description": "Local development environment"
        },
        "test_domain": {
            "pattern": "test",
            "description": "Test domains containing 'test'"
        },
        "public_domain": {
            "pattern": "*",
            "description": "All other public domains"
        }
    },
    "billing": {
        "free_tier": {
            "name": "Free Tier",
            "description": "Basic functionality with limited usage",
            "services": [
                "ai_image_generation",
                "ai_text_generation",
                "ai_seo_generation"
            ],
            "limits": {
                "test_domain": {
                    "ai_image_generation": 3,
                    "ai_text_generation": 5,
                    "ai_seo_generation": 10
                }
            }
        },
        "pro_tier": {
            "name": "Pro Tier",
            "description": "Enhanced functionality with higher limits",
            "services": [
                "ai_image_generation",
                "ai_text_generation",
                "ai_seo_generation",
                "image_optimization"
            ],
            "limits": {
                "test_domain": {
                    "ai_image_generation": 50,
                    "ai_text_generation": 100,
                    "ai_seo_generation": 200,
                    "image_optimization": 500
                }
            }
        },
        "enterprise_tier": {
            "name": "Enterprise Tier",
            "description": "Full functionality with unlimited usage",
            "services": [
                "ai_image_generation",
                "ai_text_generation",
                "ai_seo_generation",
                "image_optimization",
                "export_functionality"
            ],
            "limits": {
                "test_domain": {
                    "ai_image_generation": "unlimited",
                    "ai_text_generation": "unlimited",
                    "ai_seo_generation": "unlimited",
                    "image_optimization": "unlimited",
                    "export_functionality": "unlimited"
                }
            }
        }
    },
    "version": "1.0.0",
    "last_updated": "2024-01-15"
}